# Development Environment Variables
# Copy this file to .env.local and fill in your actual values

# Build Configuration
GENERATE_SOURCEMAP=true
CI=false
ESLINT_NO_DEV_ERRORS=false
TSC_COMPILE_ON_ERROR=false

# API Configuration
REACT_APP_API_BASE_URL=https://your-api-gateway-url.execute-api.region.amazonaws.com/dev

# AWS Cognito Configuration
REACT_APP_COGNITO_USER_POOL_ID=us-east-1_XXXXXXXXX
REACT_APP_COGNITO_CLIENT_ID=your-cognito-client-id
REACT_APP_COGNITO_DOMAIN=https://your-cognito-domain.auth.us-east-1.amazoncognito.com
REACT_APP_COGNITO_REDIRECT_SIGNIN=http://localhost:3000/
REACT_APP_COGNITO_REDIRECT_SIGNOUT=http://localhost:3000/
REACT_APP_COGNITO_REGION=us-east-1

# Site Configuration
REACT_APP_SITE_URL=http://localhost:3000